{
  "name": "cordova-plugin-indexeddb-async",
  "version": "1.0.9",
  "description": "An asynchronous IndexedDB plug-in for Cordova apps",
  "cordova": {
    "id": "cordova-plugin-indexeddb-async",
    "platforms": [
      "android",
      "ios",
      "windows",
      "browser"
    ]
  },
  "scripts": {
    "update": "npm update --save && cp node_modules/indexeddbshim/dist/indexeddbshim.min.js www/indexeddbshim.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ABB-Austin/cordova-plugin-indexeddb-async.git"
  },
  "keywords": [
    "cordova",
    "indexeddb",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-windows8",
    "cordova-windows",
    "cordova-browser"
  ],
  "author": {
    "name": "ABB Austin",
    "url": "http://abb.com"
  },
  "license": "MIT",
  "dependencies": {
    "indexeddbshim": "^1.0.10"
  }
}
